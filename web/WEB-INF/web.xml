<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">


    <servlet>
        <servlet-name>CustomerRegister</servlet-name>
        <servlet-class>com.sawan.controller.CustomerRegister</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>CustomerRegister</servlet-name>
        <url-pattern>/CustomerRegister</url-pattern>
    </servlet-mapping>



    <servlet>
        <servlet-name>CustomerLogin</servlet-name>
        <servlet-class>com.sawan.controller.CustomerLogin</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>CustomerLogin</servlet-name>
        <url-pattern>/CustomerLogin</url-pattern>
    </servlet-mapping>



    <welcome-file-list>
        <welcome-file>login.html</welcome-file>
        <welcome-file>register.html</welcome-file>
    </welcome-file-list>

</web-app>